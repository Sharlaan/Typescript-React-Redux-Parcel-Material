{
  "name": "ts-react-parcel",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Typescript with React, using parcel bundler",
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "author": "Ahn Dohyung <emaren84@gmail.com>",
  "maintainers": [
    "RaphaÃ«l Morineau <raphael.morineau@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adhrinae/ts-react-parcel"
  },
  "main": "src/index.tsx",
  "scripts": {
    "dev": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./src/index.html --open",
    "build": "npm run type-check && parcel build ./src/index.html --detailed-report --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify && npm run test"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@brigad/redux-rest-easy": "^4.0.1",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@reach/router": "^1.2.1",
    "moment": "^2.29.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "concurrently": "^4.1.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^4.0.0",
    "jest-extended": "^0.11.2",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "utility-types": "^3.7.0"
  }
}
